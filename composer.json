{
    "name": "anassrojea/laracrawler",
    "version": "1.0.0",
    "description": "Laravel sitemap generator and crawler package for SEO optimization. Supports multilingual sites, image/video indexing, link validation, priority scoring, and advanced crawling automation.",
    "keywords": [
        "laravel",
        "sitemap",
        "seo",
        "crawler",
        "laracrawler",
        "laravel-package",
        "multilingual",
        "hreflang",
        "xml-sitemap",
        "image-sitemap",
        "video-sitemap",
        "seo-tools",
        "indexability",
        "search-engine-optimization",
        "priority-scoring"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anas Shikh Al Srojieh",
            "email": "anas1544.as@gmail.com",
            "homepage": "https://www.linkedin.com/in/anas-srojea/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Anassrojea\\Laracrawler\\": "src/"
        }
    },
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/dom-crawler": "^5.0|^6.0|^7.0",
        "symfony/css-selector": "^5.0|^6.0|^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anassrojea\\Laracrawler\\LaraCrawlerServiceProvider"
            ]
        }
    }
}